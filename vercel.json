{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "cd apps/playground && pnpm build",
  "devCommand": "cd apps/playground && pnpm dev",
  "installCommand": "pnpm install",
  "framework": "nextjs",
  "git": {
    "deploymentEnabled": {
      "main": false,
      "master": false
    }
  },
  "github": {
    "autoAlias": false,
    "autoJobCancelation": true,
    "silent": true
  }
}
